<div class="interactions">
  <br>
  <% interactions.each do |interaction| %>
    <div class="column">
      <div class="space-between">
        <div><%= interaction.name %></div>
        <div><%= interaction.title %></div>
        <div><%= interaction.user.name %></div>
      </div>
      <%= interaction.description %>
    </div>
    <br>
  <% end %>
</div>
